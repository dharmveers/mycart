<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<servlet>
		<servlet-name>RegisterServlet</servlet-name>
		<display-name>RegisterServlet</display-name>
		<description></description>
		<servlet-class>mycart.servlets.RegisterServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>LoginServlet</servlet-name>
		<display-name>LoginServlet</display-name>
		<description></description>
		<servlet-class>mycart.servlets.LoginServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>LogOutServlet</servlet-name>
		<display-name>LogOutServlet</display-name>
		<description></description>
		<servlet-class>mycart.servlets.LogOutServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ProductOperationServlet</servlet-name>
		<display-name>ProductOperationServlet</display-name>
		<description></description>
		<servlet-class>mycart.servlets.ProductOperationServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>RegisterServlet</servlet-name>
		<url-pattern>/RegisterServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/LoginServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LogOutServlet</servlet-name>
		<url-pattern>/LogOutServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ProductOperationServlet</servlet-name>
		<url-pattern>/ProductOperationServlet</url-pattern>
	</servlet-mapping>
  
</web-app>
